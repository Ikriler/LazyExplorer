{
    "manifest_version": 2,
    "name": "Lazy explorer",
    "description": "Browser automation extension",
    "version": "1.0.0",
    "icons": {
        "64": "images/magnifying-glass-64.png"
    },
    "browser_action": {
        "default_popup": "/html/popup.html"
    },
    "permissons": ["activeTab", "scripting"],
    "background": {},
    "content_security_policy": "script-src 'unsafe-inline' 'unsafe-eval' 'self' blob: filesystem:"
}